{
  "name": "trufflestart",
  "version": "1.0.0",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "solhint-check": "solhint 'contracts/**/*.sol'",
    "solhint-fix": "solhint 'contracts/**/*.sol' --fix",
    "prettier-check": "prettier '**/*.{json,sol,md}' --check",
    "prettier-fix": "prettier '**/*.{json,sol,md}' --write",
    "eslint-check": "eslint '**/*.js'",
    "eslint-fix": "eslint '**/*.js' --fix",
    "test-coverage": "truffle run coverage --file='test/*.js' --network development",
    "remixd": "remixd -s $INIT_CWD --remix-ide https://remix.ethereum.org",
    "test": "truffle test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohammadkhodaie/innToken.git"
  },
